@model  Veiculo
@{
    ViewData["Title"] = "Cadastrar";
}
  

<form asp-action="Cadastrar" asp-controller="Produto">
    <div class="form-group">
        <label asp-for="Peso"></label>
        <input asp-for="Peso" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Modelo"></label>
        <input asp-for="Modelo" class="form-control" />
    </div>


    <div class="form-group">
        <label asp-for="DataFabricacao"></label>
        <input asp-for="DataFabricacao" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="TipoCombustivel"></label>
        <select class="form-control" asp-for="TipoCombustivel" asp-items="@Html.GetEnumSelectList<Combustivel>()">
               <option>Selecione</option>
        </select>
    </div>
    <botao></botao>
   

</form>

