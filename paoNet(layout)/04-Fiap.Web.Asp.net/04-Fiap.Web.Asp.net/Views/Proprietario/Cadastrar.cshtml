@model  Proprietario

    <form asp-action="Cadastrar" asp-controller="Proprietario">
        <div class="form-group">
            <label asp-for="Nome"></label>
            <input asp-for="Nome" class="form-control">
        </div>

        <div class="form-group">
            <label asp-for="Deficiente"></label>
            <input asp-for="Deficiente" class="form-control">
        </div>
        <botao texto="Cadastrar"></botao>
    </form>
