@model Jogador

    <form asp-action="Cadastrar" asp-controller="Jogador">
        <div class="form-group">
            <label asp-for="Nome"></label>
            <input asp-for="Nome" class="form-control" />
        </div>
        <div class="form-group">
            <select asp-for="TimeId" asp-items="@ViewBag.teste" class="form-control">
                <option>Selecione</option>
            </select>
        </div>
        <botao texto="Cadastrar"></botao>
    </form>